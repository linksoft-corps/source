{
    "name": "linksoft/source",
    "license": "MIT",
    "type": "linksoft-plugin",
    "description": "Market Data for SourceData",
    "keywords": [
        "LinkSoft",
        "SourceData"
    ],
    "authors": [
        {
            "name": "LinkSoft Development Corporation",
            "homepage": "https://www.linksoft.cn"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "hyperf/cache": "^2.2",
        "hyperf/guzzle": "~2.2.0",
        "psr/log": "^1.0|^2.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "LinkSoft\\Source\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LinkSoftTest\\Source\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "hyperf": {
            "config": "LinkSoft\\Source\\ConfigProvider"
        }
    }
}
